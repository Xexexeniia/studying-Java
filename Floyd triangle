public class loop {
        public static void main(String[] args) {
            int start = 1;
            for (int x = 1; x < 9; x++) {
                for (int y = 1; y < x; y++) {
                    System.out.print(start + "   ");
                    start++;
                }
                System.out.println();
            }
        }
    }
